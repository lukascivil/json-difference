{
  "name": "@monorepo/json-difference-cli",
  "version": "1.0.0",
  "author": "lukascivil",
  "homepage": "https://github.com/lukascivil/json-difference#readme",
  "description": "json-difference cli lib",
  "license": "MIT",
  "private": true,
  "main": "bin/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukascivil/json-difference.git"
  },
  "keywords": [
    "json",
    "difference",
    "cli"
  ],
  "scripts": {
    "release": "npx semantic-release --extends=./.release.config.js",
    "update-deps": "PACKAGE_NAME=$PACKAGE_NAME VERSION=$VERSION yarn run -t update-depss",
    "update-depss": "npx rjp ./package.json $PACKAGE_NAME $VERSION",
    "bump-version": "rjp package.json version $VERSION"
  },
  "bin": {
    "json-difference": "./bin/index.js"
  },
  "dependencies": {
    "json-difference": "^1.9.1",
    "yargs": "^17.7.2"
  }
}
