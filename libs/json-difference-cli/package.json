{
  "name": "json-difference-cli",
  "version": "1.0.0",
  "description": "json-difference cli",
  "main": "bin/index.js",
  "keywords": [
    "json",
    "difference",
    "cli"
  ],
  "scripts": {
    "release": "npx semantic-release --debug --extends=../../.release.config.js",
    "update-deps": "PACKAGE_NAME=$PACKAGE_NAME VERSION=$VERSION npx nx run-many -t update-depss",
    "update-depss": "npx rjp ./dist/libs/bar/package.json $PACKAGE_NAME $VERSION"
  },
  "author": "Lucas Cordeiro da Silva",
  "license": "MIT",
  "bin": {
    "json-difference": "./bin/index.js"
  },
  "dependencies": {
    "json-difference": "^1.9.1",
    "yargs": "^17.7.2"
  }
}
