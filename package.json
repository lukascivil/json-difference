{
  "name": "@monorepo/root",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "serve": "yarn nx run playground --target=serve",
    "test": "yarn nx run-many --target=test",
    "lint": "yarn nx run-many --target=lint",
    "build": "yarn nx run-many --target=build",
    "check-types": "yarn nx run-many --target=check-types",
    "graph": "yarn nx graph",
    "graph:affected": "yarn nx graph --affected",
    "release": "yarn nx run-many -t=release --parallel=1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.6.1",
    "nx": "16.5.5",
    "prettier": "^3.0.0",
    "replace-json-property": "^1.9.0",
    "semantic-release": "^21.0.7",
    "ts-jest": "^29.1.1",
    "typescript": "5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-dts": "~2.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "workspaces": {
    "packages": [
      "libs/*",
      "apps/*"
    ]
  },
  "packageManager": "yarn@3.6.1"
}
